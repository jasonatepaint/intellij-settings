      <option name="COMMAND" value="$USER_HOME$/dev/env.sh" />
      <option name="PARAMETERS" value="local.env" />
      <option name="WORKING_DIRECTORY" value="$USER_HOME$/dev" />
    </exec>
  </tool>
</toolSet>